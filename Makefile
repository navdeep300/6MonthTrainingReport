# --------------------------------------------------------------------
#  Makefile to run tex file
# -------------------------------------------------------------------

include Make.common

# --------------------------------------------------------------------
#  Targets
# -------------------------------------------------------------------

all: $(OUTPUT) $(OUTPUT)

$(OUTPUT): input macro $(INPUT)
	pdflatex $(INPUT)

# --------------------------------------------------------------------
#  make clean target
# -------------------------------------------------------------------

clean:
	rm -f input/*.aux input/*.log *.aux  *.log  *.pdf *.toc *.lof 
